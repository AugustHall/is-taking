{
  "manifest_version": 2,
  "name": "Inside RTR",
  "version": "0.0.1",

  "__default_locale": "en",
  "description": "Rent the Runway's internal tools and sites.",
  "icons": {
    "48": "./assets/icons/rtr_square.jpg"
  },

  "browser_action": {
    "default_icon": {
      "19": "./assets/icons/rtr_square.jpg",
      "38": "./assets/icons/rtr_square.jpg"
    },
    "default_title": "Inside RTR",
    "default_popup": "rtr-menu.html"
  },
  "__page_action": {},

  "author": "AHall",
  "automation": "",
  "background": {
    "scripts": [
      "./assets/javascript/popup.js"
    ],
    "persistent": false
  },
  "__background_page": "",
  "chrome_settings_overrides": {},
  "chrome_ui_overrides": {
    "bookmarks_ui": {
      "remove_bookmark_shortcut": true,
      "remove_button": true
    }
  },
  "chrome_url_overrides": {},
  "commands": {},
  "content_capabilities": "",
  "content_scripts": [
    {
      "matches": ["*://ajax.googleapis.com/*"],
      "css": ["./assets/stylesheets/main.css"],
      "js": [
        "./assets/javascript/vendor/jquery-3.1.0.min.js",
        "./assets/javascript/popup.js"
      ]
    }
  ],
  "content_security_policy": "",
  "converted_from_user_script": "",
  "current_locale": "",
  "devtools_page": "devtools.html",
  "__event_rules": [{}],
  "externally_connectable": {
    "matches": [
      "*://ajax.googleapis.com/*",
      "*://*.renttherunway.com/*"
    ]
  },
  "file_browser_handlers": [],
  "file_system_provider_capabilities": {
    "configurable": true,
    "multiple_mounts": true,
    "source": "network"
  },
  "homepage_url": "https://www.renttherunway.com",
  "__import": [{"id": ""}],
  "__incognito": "spanning, split, or not_allowed",
  "input_components": "",
  "__key": "",
  "minimum_chrome_version": "50",
  "nacl_modules": [],
  "__oauth2": "",
  "offline_enabled": true,
  "omnibox": {
    "keyword": "aString"
  },
  "optional_permissions": ["tabs"],
  "__options_page": "options.html",
  "__options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": ["tabs"],
  "platforms": "",
  "plugins": [],
  "requirements": {},
  "sandbox": [],
  "short_name": "Short Name",
  "signature": "",
  "spellcheck": "",
  "__storage": {
    "managed_schema": "schema.json"
  },
  "system_indicator": "",
  "tts_engine": {},
  "update_url": "http://path/to/updateInfo.xml",
  "version_name": "Beginning",
  "web_accessible_resources": []
}
